{
  "name": "toml.io",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn parse && webpack && postcss --verbose code/stylesheets/application.pcss -o public/stylesheets/application.css",
    "clean": "del 'public/!(images|favicon.*)'",
    "dev": "$npm_execpath serve & $npm_execpath watch",
    "netlify": "$npm_execpath watch & netlify dev",
    "parse": "node lib/parse_spec.js",
    "rebuild": "$npm_execpath clean && $npm_execpath build",
    "serve": "live-server --watch=./public --mount=/:./public --entry-file='public/404.html'",
    "watch": "webpack --watch & postcss --verbose code/stylesheets/application.pcss -o public/stylesheets/application.css --watch"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "del-cli": "^3.0.0",
    "live-server": "^1.2.1"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@octokit/rest": "^16.28.9",
    "autoprefixer": "^9.6.1",
    "cameronjs": "^0.4.2",
    "html-entities": "^1.2.1",
    "marked": "^0.7.0",
    "node-fetch": "^2.6.0",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-nesting": "^7.0.1",
    "stimulus": "^1.1.1",
    "string-dasherize": "^1.0.0",
    "striptags": "^3.1.1",
    "tailwindcss": "^1.1.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  }
}
