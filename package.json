{
  "name": "toml.io",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack && postcss --verbose code/stylesheets/application.pcss -o public/stylesheets/application.css",
    "clean": "del 'public/!(images)'",
    "develop": "$npm_execpath serve & $npm_execpath watch",
    "rebuild": "$npm_execpath clean && $npm_execpath build",
    "serve": "live-server --watch=./public --mount=/:./public --entry-file='public/404.html'",
    "watch": "webpack --watch & postcss --verbose code/stylesheets/application.pcss -o public/stylesheets/application.css --watch"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.6.1",
    "del-cli": "^3.0.0",
    "file-include-webpack-plugin": "https://github.com/cannikin/file-include-webpack-plugin.git#three-level-fix",
    "live-server": "^1.2.1",
    "markdown-it": "^10.0.0",
    "node-fetch": "^2.6.0",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-nesting": "^7.0.1",
    "remove-files-webpack-plugin": "^1.1.3",
    "stimulus": "^1.1.1",
    "string-dasherize": "^1.0.0",
    "tailwindcss": "^1.1.2",
    "webpack": "^4.39.3"
  },
  "dependencies": {
    "webpack-cli": "^3.3.8"
  }
}
